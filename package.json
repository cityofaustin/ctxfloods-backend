{
  "name": "ctxfloods-backend",
  "version": "0.0.1",
  "description": "CTXFloods serverless backend",
  "main": "handler.js",
  "scripts": {
    "setup-local": "bash ./deployment/runForEnv.sh ./db/scripts/migrateAndSeed.js local",
    "start-local": "yarn migrate-local; yarn build-local-schema; yarn start-express-server",
    "start-express-server": "bash ./deployment/runForEnv.sh ./localServer.js local",
    "test": "yarn init-local-db; yarn drop-local-data; yarn migrate-local; yarn build-local-schema; yarn add-test-data; yarn start-express-server & JWT_SECRET='insecure' BACKEND_PORT=5000 react-scripts test",
    "init-local-db": "bash ./deployment/runForEnv.sh ./db/scripts/initialize.js local",
    "drop-local-data": "bash ./deployment/runForEnv.sh ./db/scripts/dropFloodsData.js local",
    "migrate-local": "bash ./deployment/runForEnv.sh ./db/scripts/migrate.js local",
    "build-local-schema": "bash ./deployment/runForEnv.sh ./pgCatalog/buildPgCatalog.js local",
    "add-test-data": "bash ./deployment/runForEnv.sh ./db/scripts/addTestData.js local",
    "seed-local": "bash ./deployment/runForEnv.sh ./db/scripts/seed.js local",
    "destroy-local-db": "bash ./deployment/runForEnv.sh ./db/scripts/destroyFloodsDb.js local"
  },
  "author": "Richard",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "d3-dsv": "^1.0.8",
    "google-recaptcha": "^1.1.0",
    "graphql": "^0.8.2",
    "handlebars": "^4.0.11",
    "jsonwebtoken": "^8.2.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "nodemailer": "^4.6.3",
    "pg-minify": "^0.4.2",
    "postgraphql": "^2.5.0",
    "postgres-migrations": "^2.0.1",
    "prettier": "^1.10.2",
    "promise.prototype.finally": "^3.1.0",
    "raven": "^2.6.3",
    "react-scripts": "^1.0.10",
    "uuid": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "aws-sdk": "^2.212.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "serverless-content-encoding": "^1.0.22"
  }
}
