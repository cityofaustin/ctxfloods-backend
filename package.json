{
  "name": "ctxfloods-backend",
  "version": "0.0.1",
  "description": "CTXFloods serverless backend",
  "main": "handler.js",
  "config": {
    "PGLOCALCON": "postgresql://postgres@localhost:5432/floods",
    "PGLOCALRUNCON": "postgresql://floods_postgraphql@localhost:5432/floods",
    "PGLOCALINITCON": "postgresql://postgres@localhost:5432/",
    "FRONTEND_URL_LOCAL": "localhost:3000",
    "BACKEND_PORT_LOCAL": "5000"
  },
  "scripts": {
    "start-local": "yarn local-source; yarn migrate-local; yarn build-local-schema; yarn start-express-server",
    "start-express-server": "yarn local-source; node localServer.js",
    "test": "yarn drop-local-data; yarn migrate-local; yarn build-local-schema; yarn add-test-data; yarn start-express-server & JWT_SECRET='insecure' BACKEND_PORT=$npm_package_config_BACKEND_PORT_LOCAL react-scripts test",
    "migrate": "node populateDB/migrate.js",
    "migrate-local": "yarn local-source; node populateDB/migrate.js",
    "add-setup-data-local": "psql $npm_package_config_PGLOCALCON -f ./populateDB/addSetupData.sql",
    "add-communities-local": "psql $npm_package_config_PGLOCALCON -f ./populateDB/data/addCommunities.sql",
    "add-waze-streets-local": "BACKEND_PORT=$npm_package_config_BACKEND_PORT_LOCAL node ./populateDB/data/addWazeStreets.js",
    "add-crossings-local": "BACKEND_PORT=$npm_package_config_BACKEND_PORT_LOCAL node ./populateDB/data/addLegacyCrossings.js",
    "init-local-db": "psql $npm_package_config_PGLOCALINITCON -f ./populateDB/initLocal.sql",
    "drop-local-data": "yarn local-source; psql $npm_package_config_PGLOCALCON -f ./populateDB/drop.sql",
    "build-schema": "node ./pgCatalog/buildPgCatalog.js $npm_config_PGCON floods",
    "build-local-schema": "node ./pgCatalog/buildPgCatalog.js floods",
    "add-test-data": "psql $npm_package_config_PGLOCALCON -f ./populateDB/testing/addTestData.sql",
    "local-source": "source ./deployment/vars/local.sh",
    "nick-init": "yarn local-source; node ./seed/initialize.js"
  },
  "author": "Richard",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "csv": "^3.0.2",
    "d3-dsv": "^1.0.8",
    "google-recaptcha": "^1.1.0",
    "graphql": "^0.8.2",
    "handlebars": "^4.0.11",
    "jsonwebtoken": "^8.2.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "nodemailer": "^4.6.3",
    "pg-minify": "^0.4.2",
    "pg-promise": "^8.4.6",
    "postgraphql": "^2.5.0",
    "postgres-migrations": "^2.0.1",
    "prettier": "^1.10.2",
    "raven": "^2.6.3",
    "react-scripts": "^1.0.10",
    "uuid": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "serverless-content-encoding": "^1.0.22"
  }
}
